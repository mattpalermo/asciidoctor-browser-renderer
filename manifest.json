{
  "manifest_version": 2,
  "name": "Asciidoctor extension experiment",
  "description": "Simple asciidoctor browser extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches":[
        "*://*/*.ad",
        "*://*/*.ad?*",
        "*://*/*.ad.*",
        "*://*/*.adoc",
        "*://*/*.adoc?*",
        "*://*/*.adoc.*",
        "*://*/*.asc",
        "*://*/*.asc?*",
        "*://*/*.asc.*",
        "*://*/*.asciidoc",
        "*://*/*.asciidoc?*",
        "*://*/*.asciidoc.*",
        "file://*/*.ad",
        "file://*/*.ad?*",
        "file://*/*.ad.*",
        "file://*/*.adoc",
        "file://*/*.adoc?*",
        "file://*/*.adoc.*",
        "file://*/*.asc",
        "file://*/*.asc?*",
        "file://*/*.asc.*",
        "file://*/*.asciidoc",
        "file://*/*.asciidoc?*",
        "file://*/*.asciidoc.*"
      ],
      "js":[
        "asciidoctor.js",
        "main.js"
      ],
      "css":[
        "asciidoctor.css"
      ]
    }
  ]
}
